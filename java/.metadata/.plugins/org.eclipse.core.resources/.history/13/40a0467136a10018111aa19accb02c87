package helloWorld;


public class Mthod_test {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		int eigo = 78;
		int sugaku = 90;
		int kokugo = 68;
		check("英語", eigo);
		check("数学", sugaku);
		check("国語", kokugo);
	}

	private static void check(String kyoka, int seiseki) {
		// TODO 自動生成されたメソッド・スタブ
		for (int i = 0; i < kyoka.length; i++) {
			System.out.println(kyoka[i] + seiseki[i]);
		}
		
	}

}
