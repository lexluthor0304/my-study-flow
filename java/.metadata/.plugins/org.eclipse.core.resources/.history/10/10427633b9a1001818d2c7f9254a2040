package helloWorld;

public class Mthod2 {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		int num = 8;
		int array[] = {10, 4};
		System.out.println("num = " + num);
		System.out.println("array[0] = " + array[0]);
		//henkou(num, array);
		num = henkou(num, array);
		System.out.println("num = " + num);
		System.out.println("array[0] = " + array[0]);
	}

	private static int henkou(int num, int[] array) {
		// TODO 自動生成されたメソッド・スタブ
		num = 5;
		array[0] = 12;
		return num;
	}

}
