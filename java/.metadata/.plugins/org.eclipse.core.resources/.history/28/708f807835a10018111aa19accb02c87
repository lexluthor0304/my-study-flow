package helloWorld;


public class Mthod1 {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		int eigo = 78;
		int sugaku = 90;
		int kokugo = 68;
		check("英語", eigo);
		check("数学", sugaku);
		check("国語", kokugo);
	}

	private static void check(String kyoka, int seiseki) {
		// TODO 自動生成されたメソッド・スタブ
		System.out.println(kyoka + "の試験結果は");
		if (seiseki > 80) {
			System.out.println("合格");
		}else {
			System.out.println("不合格");
		}
	}

}
