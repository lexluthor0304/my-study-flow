package helloWorld;

public class Learning_break {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		outside: for (int i = 1; i <5; i++) {
			for (int j = 1; j < 5; j++) {
				if (i * j > 10) {
					break outside;
				}
				System.out.println(i + "*" + j + "=" + (i*j));
			}
			System.out.println(i);
		}
	}

}
